let paisDeOrigem 

if (paisDeOrigem === "Brasil") {
    console.log("brasileiro")
}else if (paisDeOrigem === "EUA"){
    console.log("estadunidense")
}else if (paisDeOrigem === "Inglaterra"){
    console.log("ingles")
}else if (paisDeOrigem === "Canada"){
    console.log("canadense")
}else {
    console.log("Nacionalidade não encontrada")
}
--------------------------------------------------
let nacionalidade
switch (nacionalidade) {
    case "Brasil":
        console.log("brasileiro")        
        break
case "EUA":
    console.log("estadunidense")
    break
    case "Inglaterra":
        console.log("Ingles")
        break
        case "Canada":
            console.log("canadense")
            default:
                console.log("Nacionalidade não encontrada")
        break;
}
--------------------------------------------------------------

let pokemonInicial

switch (pokemonInicial) {
    case "Bulbasauro": 
        console.log("Você escolheu o Bulbasauro como seu pokémon inicial")

        break

        case "Charmander":
        console.log("Você escolheu Charmander como seu pokémon inicial")

        break

case "Squirtle":
    Console.log("Você escolheu Squirtle como seu pokémon inicial")
        break;
}
